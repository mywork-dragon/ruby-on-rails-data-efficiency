    .row.d-none.d-lg-flex
      .mx-auto.chart-container#container style="min-width: 1010px"
        #inner
          span
            | Top 100 SDKS by
          span class="btn-group" style="margin-right: 7px"
            button.btn-light.ml-2 type="button" id="fastest-growing-sdk-chart-abs" class="btn btn-default btn-primary" aria-pressed="true"
